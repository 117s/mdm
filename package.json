{
  "name": "@117cloud/mdm",
  "version": "0.1.0-snapshot.0",
  "main": "index.js",
  "repository": "git@github.com:117s/mdm.git",
  "author": "zhaochy <zhaochy_2015@hotmail.com>",
  "license": "Apache 2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "startPG": "docker run -e POSTGRES_PASSWORD=auth -e POSTGRES_USER=postgres -p 5432:5432 --name postgres -d postgres",
    "gen-swagger": "swag init",
    "fmt": "swag fmt && swag fmt && go fmt",
    "semantic-release": "semantic-release"
  },
  "files": ["package.json"],
  "release": {
    "branches": ["master", { "name": "release", "range": "1.x" }],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/git"
    ]
  }
}
